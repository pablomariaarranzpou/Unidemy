<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unidemy" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/common_google_signin_btn_icon_dark"
        android:label="@string/app_name"
        android:roundIcon="@drawable/common_google_signin_btn_icon_dark"
        android:supportsRtl="true"
        android:theme="@style/Theme.Unidemy" >
        <activity
            android:name=".ui.Register"
            android:exported="false"
            android:label="@string/app_name" />
        <activity android:name=".ui.FirstLogin"
            android:exported="false"
            android:label="@string/app_name" />
        <activity
            android:name=".ui.Login"
            android:exported="true"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>